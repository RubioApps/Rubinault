                },
                {
                    "key": "target",
                    "value": "horn"
                }
            ],
            "response": {
                "code": 403
            }
        },
        "Lights": {
            "name": "actions/horn-lights",
            "version": 1,
            "adapter": "kca/car-adapter",
            "payload": [
                {
                    "key": "action",
