{
    "Endpoint": {
        "Cockpit": [
            {
                "name": "cockpit",
                "version": 1,
                "adapter": "kca/car-adapter",
                "response": {
                    "code": 200,
                    "path": "data/attributes",
                    "dictionary": {
                        "autonomy": "fuelAutonomy",
                        "remaining": "fuelQuantity",
                        "mileage": "totalMileage"
                    }
                }
            }
        ],
        "Location": [
            {
                "name": "location",
                "version": 1,
                "adapter": "kca/car-adapter",
                "response": {
                    "code": 200,
                    "path": "data/attributes",
                    "dictionary": {
                        "latitude": "gpsLatitude",
                        "longitude": "gpsLongitude",
                        "timestamp": "lastUpdateTime"
                    }
                }
            }
        ],
        "LockStatus": [
            {
                "name": "lock-status",
                "version": 1,
                "adapter": "kca/car-adapter",
                "response": {
                    "code": 404,
                    "path": "data/attributes",
                    "dictionary": {}
                }
            }
        ]              
    },
    "Action": {
        "Horn": {
            "name": "actions/horn-lights",
            "version": 1,
            "adapter": "kca/car-adapter",
