                },
                {
                    "key": "target",
                    "value": "lights"
                }
            ],
            "response": {
                "code": 403
            }
        },
        "HornAndLights": {
            "name": "actions/horn-lights",
            "version": 1,
            "adapter": "kca/car-adapter",
            "payload": [
                {
                    "key": "action",
                    "value": "start"
                },
                {
                    "key": "target",
                    "value": "horn_lights"
                }
            ],
            "response": {
                "code": 403
            }
        },
        "RefreshLocation": {
